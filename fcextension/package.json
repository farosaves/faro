{
	"name": "fce2",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && node removeInlineScript.cjs",
		"build-crx": "vite build --mode crx",
		"reload": "cp -r build/* dist && sleep 0.1 && cp -r dist/* build && sleep 0.5 && open \"http://reload.extensions\"",
		"bz": "npm run build && npm run build-crx && zip -r ../supabase-sveltekit/static/ext.zip build",
		"bfull": "npm run build && npm run build-crx && npm run reload",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test": "vitest",
		"update-types": "(npx supabase gen types typescript --project-id \"hspfduumtusknleyjeoi\" --schema public > src/lib/dbtypes.ts) && (npx better-supabase-types -i src/lib/dbtypes.ts -f)",
		"zip": "zip -r ../supabase-sveltekit/static/ext.zip build"
	},
	"devDependencies": {
		"@crxjs/vite-plugin": "^2.0.0-beta.23",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/chrome": "^0.0.256",
		"@types/eslint": "8.56.0",
		"@types/jsdom": "^21.1.6",
		"@types/node": "^20.11.17",
		"@types/rangy": "^0.0.38",
		"@types/sbd": "^1.0.5",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"chrome-types": "^0.1.247",
		"daisyui": "^4.5.0",
		"dotenv": "^16.3.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"jsdom": "^24.0.0",
		"postcss": "^8.4.33",
		"rangy": "^1.3.1",
		"supabase": "^1.136.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"sveltekit-adapter-chrome-extension": "github:asjir/sveltekit-adapter-chrome-extension",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^1.3.1"
	},
	"type": "module",
	"dependencies": {
		"@supabase/ssr": "^0.0.10",
		"@trpc/client": "^10.45.0",
		"fp-ts": "^2.16.2",
		"sbd": "^1.0.19",
		"svelte-persisted-store": "^0.9.0",
		"trpc-sveltekit": "^3.5.22"
	}
}
