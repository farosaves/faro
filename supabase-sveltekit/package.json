{
  "name": "supabase-sveltekit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "open http://localhost:5173/ & vite dev",
    "build": "vite build",
    "b": "npm install --prefix ../shared && npm run build --prefix ../shared && npm run build"
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "update-types": "(npx supabase gen types typescript --project-id \"hspfduumtusknleyjeoi\" --schema public > src/lib/dbtypes.ts) && (npx better-supabase-types -i src/lib/dbtypes.ts -f)",
    "test": "vitest",
    "sync": "cd ..; julia --project=oxygen oxygen/syncer.jl",
    "diff": "diff --color -bur ../fcextension/src/lib/shared src/lib/shared"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.1.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "8.56.0",
    "@types/jsdom": "^21.1.6",
    "@types/pako": "^2.0.3",
    "@types/rangy": "^0.0.38",
    "@types/sbd": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.4.24",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "marklib": "^0.11.0",
    "postcss": "^8.4.32",
    "rangy": "^1.3.1",
    "supabase-ui-svelte": "^0.1.1",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "ts-semaphore": "^1.0.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^1.1.3"
  },
  "type": "module",
  "dependencies": {
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/auth-ui-svelte": "^0.2.8",
    "@supabase/ssr": "^0.0.10",
    "@supabase/supabase-js": "^2.39.1",
    "@trpc/client": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "@xenova/transformers": "^2.15.0",
    "date-fns": "^3.3.1",
    "fp-ts": "^2.16.2",
    "fsrs.js": "^1.2.0",
    "fuzzysort": "^2.0.4",
    "jsdom": "^24.0.0",
    "replicate": "^0.25.2",
    "sbd": "^1.0.19",
    "shared": "file:../shared",
    "svelte-forms": "^2.3.1",
    "svelte-formula": "^0.12.0",
    "svelte-hero-icons": "^5.1.0",
    "svelte-persisted-store": "^0.9.1",
    "svelte-tags-input": "^6.0.0",
    "theme-change": "^2.5.0",
    "trpc-sveltekit": "^3.5.22",
    "ts-pattern": "^5.0.8",
    "zod": "^3.22.4"
  }
}
