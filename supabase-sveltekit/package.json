		"fsrs.js": "^1.2.0",
		"fuse.js": "^7.0.0",
		"fuzzysort": "^2.0.4",
		"jsdom": "^24.0.0",
		"lz-string": "^1.5.0",
		"openai": "^4.24.7",
		"pako": "^2.1.0",
		"rangy": "^1.3.1",
