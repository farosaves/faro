{
	"name": "supabase-sveltekit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "open http://localhost:5173/ & vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"update-types": "(npx supabase gen types typescript --project-id \"hspfduumtusknleyjeoi\" --schema public > src/lib/dbtypes.ts) && (npx better-supabase-types -i src/lib/dbtypes.ts -f)",
		"test": "vitest",
		"sync": "cd ..; julia --project=oxygen oxygen/syncer.jl",
		"diff": "diff --color -bur ../fcextension/src/lib/shared src/lib/shared"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.1.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "8.56.0",
		"@types/pako": "^2.0.3",
		"@types/rangy": "^0.0.38",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"marklib": "^0.11.0",
		"postcss": "^8.4.32",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.1",
		"supabase-ui-svelte": "^0.1.1",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.0",
		"ts-node": "^10.9.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^1.1.3"
	},
	"type": "module",
	"dependencies": {
		"@rangy/classapplier": "^2.1.0",
		"@rangy/highlighter": "^2.1.0",
