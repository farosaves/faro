{
  "name": "shared",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "watch": "npm-watch",
    "update-types": "(npx supabase gen types typescript --project-id \"hspfduumtusknleyjeoi\" --schema public > src/lib/dbtypes.ts) && (npx better-supabase-types -i src/lib/dbtypes.ts -f) && (npx supabase-to-zod --input src/lib/dbtypes.ts --output src/lib/schemas.ts)"
  },
  "watch": {
    "build": {
      "patterns": [
        "src/lib"
      ],
      "extensions": [
        "ts",
        "svelte"
      ],
      "commands": [
        "npm run build"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/jsdom": "^21.1.6",
    "@types/sbd": "^1.0.5",
    "autoprefixer": "^10.4.18",
    "daisyui": "^4.7.3",
    "postcss": "^8.4.35",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "publint": "^0.1.9",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.11",
    "vitest": "^1.2.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.7",
    "fp-ts": "^2.16.2",
    "fuzzysort": "^2.0.4",
    "immer": "^10.0.3",
    "jsdom": "^24.0.0",
    "npm-watch": "^0.11.0",
    "rxjs": "^7.8.1",
    "sbd": "^1.0.19",
    "structurajs": "^0.12.0",
    "svelte-persisted-store": "^0.9.1"
  }
}
