{
  "name": "sextension",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "bnr": "sh misc/devon; VITE_PI_IP=http://localhost:5173 vite build; sh misc/devoff",
    "b": "bnr && sleep 0.5 && open \"http://reload.extensions\"",
    "preview": "vite preview",
    "watch": "npm-watch",
    "bz": "VITE_PI_IP=https://farosapp.com VITE_DEBUG=false vite build && rm ../supabase-sveltekit/static/ext.zip && zip -r ../supabase-sveltekit/static/ext.zip dist",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "watch": {
    "bnr": {
      "patterns": [
        "src",
        "../shared/dist"
      ],
      "extensions": [
        "ts",
        "svelte",
        "js"
      ]
    }
  },
  "devDependencies": {
    "@iconify-json/tabler": "^1.1.109",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tsconfig/svelte": "^5.0.2",
    "autoprefixer": "^10.4.18",
    "daisyui": "^4.7.3",
    "postcss": "^8.4.35",
    "svelte": "^4.2.11",
    "svelte-check": "^3.6.4",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "unplugin-icons": "^0.18.5",
    "vite": "^5.1.4"
  },
  "dependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.23",
    "@extend-chrome/messages": "^1.2.2",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.39.8",
    "@trpc/client": "^10.45.1",
    "@types/chrome": "^0.0.262",
    "@types/rangy": "^0.0.38",
    "devalue": "^4.3.2",
    "fp-ts": "^2.16.2",
    "fp-ts-std": "^0.18.0",
    "npm-watch": "^0.11.0",
    "rangy": "^1.3.1",
    "rxjs": "^7.8.1",
    "shared": "file:../shared",
    "trpc-chrome": "^1.0.0"
  }
}
